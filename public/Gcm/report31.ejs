<h1>Detailed Report Based On Grievance Type</h1>
<div ng-app="grievances" ng-controller="Gcm">
<div class="form-group">
   <div class="row">
    <div class="col-sm-2 col-sm-offset-1">
       <label class="control-label"> From Date</label>
   </div>
   <div class="col-sm-7">
       <input class="form-control" type="date" name="from-date" value="2018-10-10">
   </div>
</div>
</div>
<div class="form-group">
    <div class="row">
        <div class="col-sm-2 col-sm-offset-1">
           <label class="control-label"> To Date</label>
       </div>
       <div class="col-sm-7">
           <input class="form-control" type="date" name="to-date" value="2018-10-10">
       </div>
   </div>
</div>
<div class="form-group">
    <div class="row">
        <div class="col-sm-2 col-sm-offset-1">
           <label class="control-label">Grievance Type</label>
       </div>
       <div class="col-sm-7">
        <select ng-model="Gtype">
            <option value="">Select One</option>
            <!--<option ng-repeat="obj in grievance_type" value="{{obj.seq}}&{{obj.grvtype}}">{{obj.grvtype}}</option>-->
        </select>
    </div>
</div>
</div>
<div class="form-group">
    <div class="col-sm-7 col-sm-offset-3">
        <button>
            <a class="btn btn-default" href="reports.ejs" download="Reports"> Create</a>
        </button>
    </div>
</div>
</div>
